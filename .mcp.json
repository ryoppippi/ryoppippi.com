{
	"mcpServers": {
		"context7": {
			"type": "http",
			"url": "https://mcp.context7.com/mcp"
		},
		"grep": {
			"type": "http",
			"url": "https://mcp.grep.app"
		},
		"chrome-devtools": {
			"type": "stdio",
			"command": "pnpm",
			"args": [
				"chrome-devtools-mcp"
			],
			"env": {}
		},
		"svelte": {
			"type": "stdio",
			"command": "pnpm",
			"args": [
				"svelte-mcp"
			],
			"env": {}
		}
	}
}
