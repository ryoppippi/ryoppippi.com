{
	"name": "ryoppippi.com",
	"type": "module",
	"version": "0.0.1",
	"private": true,
	"engines": {
		"npm": "use bun please!",
		"yarn": "use bun please!"
	},
	"scripts": {
		"dev": "vite dev",
		"build": "vite build",
		"preview": "vite preview",
		"sync": "svelte-kit sync",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"lint": "eslint --cache .",
		"format": "nr lint --fix",
		"prepare": "nlx bunpare && nr prepare:typia && nr prepare:githooks",
		"prepare:typia": "ts-patch install",
		"prepare:githooks": "git config --local core.hooksPath .githooks || echo 'Can not set git hooks'"
	},
	"devDependencies": {
		"@antfu/ni": "^0.22.0",
		"@ryoppippi/eslint-config": "npm:@jsr/ryoppippi__eslint-config",
		"@ryoppippi/unplugin-typia": "npm:@jsr/ryoppippi__unplugin-typia",
		"@ryoppippi/vite-plugin-cloudflare-redirect": "npm:@jsr/ryoppippi__vite-plugin-cloudflare-redirect",
		"@secretlint/secretlint-rule-preset-recommend": "^8.2.4",
		"@sveltejs/adapter-auto": "^3.2.2",
		"@sveltejs/adapter-static": "^3.0.2",
		"@sveltejs/enhanced-img": "^0.3.1",
		"@sveltejs/kit": "^2.5.18",
		"@sveltejs/vite-plugin-svelte": "^3.1.1",
		"@total-typescript/ts-reset": "^0.5.1",
		"@types/markdown-it": "^14.1.1",
		"@types/node": "^20.14.10",
		"@unocss/extractor-svelte": "^0.61.4",
		"@unplugin/macros": "npm:@jsr/unplugin__macros",
		"@wooorm/starry-night": "^3.4.0",
		"autoprefixer": "^10.4.20",
		"buffer": "^6.0.3",
		"date-fns": "^3.6.0",
		"eslint": "^9.8.0",
		"eslint-plugin-format": "^0.1.2",
		"eslint-plugin-svelte": "^2.42.0",
		"favicons": "^7.2.0",
		"gray-matter": "^4.0.3",
		"markdown-it": "^14.1.0",
		"rss-parser": "^3.13.0",
		"secretlint": "^8.2.4",
		"sort-on": "^6.0.0",
		"std-env": "^3.7.0",
		"svelte": "^5.0.0-next.184",
		"svelte-check": "^3.8.5",
		"svelte-meta-tags": "^3.1.2",
		"svelte-preprocess-import-assets": "^1.1.0",
