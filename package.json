		"tslib": "^2.6.3",
		"typescript": "~5.5.3",
		"typescript-svelte-plugin": "^0.3.39",
		"typia": "^6.5.1",
		"ufo": "^1.5.3",
		"unocss": "^0.61.4",
		"vite": "^5.3.3"
	},
	"trustedDependencies": [
		"@sveltejs/kit",
		"esbuild",
		"sharp",
		"svelte-preprocess"
	]
}
